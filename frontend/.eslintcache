nd\\src\\actions\\productActions.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\CartScreen.js",["110"],"import React, { useEffect } from 'react';\nimport {\n  Row,\n  Col,\n  Image,\n  Form,\n  Button,\n  Card,\n  ListGroup,\n  Container,\n} from 'react-bootstrap';\nimport { addToCart, removeFromCart } from '../actions/cartActions';\nimport { useDispatch, useSelector } from 'react-redux';\nimport { Link } from 'react-router-dom';\nimport Message from '../components/Message';\n\nconst CartScreen = ({ match, location, history }) => {\n  // getting the product id from url\n  const productId = match.params.id;\n\n  // getting the quanity\n  // ?qty=1  getting the number with split('=') after this is the number\n  const qty = location.search ? Number(location.search.split('=')[1]) : 1;\n\n  // dispatch for calling the action\n  const dispatch = useDispatch();\n\n  // getting data\n  const cart = useSelector(state => state.cart);\n  const { cartItems } = cart;\n\n  const removeItem = id => {\n    dispatch(removeFromCart(id));\n  };\n\n  const submitHandler = () => {\n    history.push('/login?redirect=shipping');\n  };\n\n  const redirectHandler = () => {\n    history.push('/');\n  };\n\n  useEffect(() => {\n    if (productId) {\n      dispatch(addToCart(productId, qty));\n    }\n  }, [dispatch, productId, qty]);\n  return (\n    <Row>\n      <Col md={8}>\n        <h1>Your items</h1>\n        {cartItems.length === 0 ? (\n          <Message>\n            Your cart is empty <Link to='/'>Go Back</Link>\n          </Message>\n        ) : (\n          <ListGroup variant='flush'>\n            {cartItems.map(item => (\n              <ListGroup.Item key={item.product}>\n                <Row>\n                  <Col md={3}>\n                    <Link to={`/product/${item.product}`}>\n                      <h4>{item.name}</h4>\n                    </Link>\n                  </Col>\n                  <Col md={4}>\n                    <Link to={`/product/${item.product}`}>\n                      <Image src={item.image} alt={item.name} fluid />\n                    </Link>\n                  </Col>\n                  <Col md={2}>\n                    <Form.Control\n                      as='select'\n                      value={item.qty}\n                      onChange={e =>\n                        dispatch(\n                          addToCart(item.product, Number(e.target.value))\n                        )\n                      }\n                    >\n                      {[...Array(item.countInStock).keys()].map(i => (\n                        <option key={i + 1} value={i + 1}>\n                          {i + 1}\n                        </option>\n                      ))}\n                    </Form.Control>\n                  </Col>\n                  <Col md={3}>\n                    <h2> {item.price} €</h2>\n                    <Button\n                      size='sm'\n                      type='button'\n                      onClick={() => removeItem(item.product)}\n                    >\n                      {' '}\n                      <i className='fas fa-trash'></i>\n                    </Button>\n                  </Col>\n                </Row>\n              </ListGroup.Item>\n            ))}\n          </ListGroup>\n        )}\n      </Col>\n\n      {/* Total */}\n\n      <Col md={3}>\n        <Card>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h3>\n                Total items:\n                {cartItems.reduce(\n                  (accumulater, current) => accumulater + current.qty,\n                  0\n                )}\n              </h3>\n              <h5>Total Price:</h5>\n              <strong>\n                {cartItems\n                  .reduce(\n                    (accumulater, current) =>\n                      accumulater + current.qty * current.price,\n                    0\n                  )\n                  .toFixed(2)}\n                €\n              </strong>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <Button\n                type='button'\n                className='btn-block'\n                disabled={cartItems.length === 0}\n                onClick={submitHandler}\n              >\n                Order\n              </Button>\n            </ListGroup.Item>\n          </ListGroup>\n        </Card>\n      </Col>\n      <Col md={3}>\n        <Button type='button' className='btn-block' onClick={redirectHandler}>\n          Back to shopping\n        </Button>\n      </Col>\n    </Row>\n  );\n};\n\nexport default CartScreen;\n","E:\\bestdeveloper\\webshop\\frontend\\src\\reducers\\cartReducers.js",["111"],"E:\\bestdeveloper\\webshop\\frontend\\src\\constants\\cartConstants.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\actions\\cartActions.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\Message.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\Loader.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\reducers\\userReducers.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\constants\\userConstants.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\LoginScreen.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\FormSkeleton.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\actions\\userActions.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\RegisterScreen.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\ShippingScreen.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\PaymentScreen.js",[],{"ruleId":"112","replacedBy":"113"},{"ruleId":"114","replacedBy":"115"},{"ruleId":"116","severity":1,"message":"117","line":10,"column":3,"nodeType":"118","messageId":"119","endLine":10,"endColumn":12},{"ruleId":"120","severity":1,"message":"121","line":53,"column":7,"nodeType":"122","messageId":"123","endLine":53,"endColumn":13},"no-native-reassign",["124"],"no-negated-in-lhs",["125"],"no-unused-vars","'C