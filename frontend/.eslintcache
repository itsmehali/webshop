y = location.search ? Number(location.search.split('=')[1]) : 1;\n\n  // dispatch for calling the action\n  const dispatch = useDispatch();\n\n  // getting data\n  const cart = useSelector(state => state.cart);\n  const { cartItems } = cart;\n\n  const removeItem = id => {\n    dispatch(removeFromCart(id));\n  };\n\n  const submitHandler = () => {\n    history.push('/login?redirect=shipping');\n  };\n\n  const redirectHandler = () => {\n    history.push('/');\n  };\n\n  useEffect(() => {\n    if (productId) {\n      dispatch(addToCart(productId, qty));\n    }\n  }, [dispatch, productId, qty]);\n  return (\n    <Row>\n      <Col md={8}>\n        <h1>Your items</h1>\n        {cartItems.length === 0 ? (\n          <Message>\n            Your cart is empty <Link to='/'>Go Back</Link>\n          </Message>\n        ) : (\n          <ListGroup variant='flush'>\n            {cartItems.map(item => (\n              <ListGroup.Item key={item.product}>\n                <Row>\n                  <Col md={3}>\n                    <Link to={`/product/${item.product}`}>\n                      <h4>{item.name}</h4>\n                    </Link>\n                  </Col>\n                  <Col md={4}>\n                    <Link to={`/product/${item.product}`}>\n                      <Image src={item.image} alt={item.name} fluid />\n                    </Link>\n                  </Col>\n                  <Col md={2}>\n                    <Form.Control\n                      as='select'\n                      value={item.qty}\n                      onChange={e =>\n                        dispatch(\n                          addToCart(item.product, Number(e.target.value))\n                        )\n                      }\n                    >\n                      {[...Array(item.countInStock).keys()].map(i => (\n                        <option key={i + 1} value={i + 1}>\n                          {i + 1}\n                        </option>\n                      ))}\n                    </Form.Control>\n                  </Col>\n                  <Col md={3}>\n                    <h2> {item.price} €</h2>\n                    <Button\n                      size='sm'\n                      type='button'\n                      onClick={() => removeItem(item.product)}\n                    >\n                      {' '}\n                      <i className='fas fa-trash'></i>\n                    </Button>\n                  </Col>\n                </Row>\n              </ListGroup.Item>\n            ))}\n          </ListGroup>\n        )}\n      </Col>\n\n      {/* Total */}\n\n      <Col md={3}>\n        <Card>\n          <ListGroup variant='flush'>\n            <ListGroup.Item>\n              <h3>\n                Total items:\n                {cartItems.reduce(\n                  (accumulater, current) => accumulater + current.qty,\n                  0\n                )}\n              </h3>\n              <h5>Total Price:</h5>\n              <strong>\n                {cartItems\n                  .reduce(\n                    (accumulater, current) =>\n                      accumulater + current.qty * current.price,\n                    0\n                  )\n                  .toFixed(2)}\n                €\n              </strong>\n            </ListGroup.Item>\n            <ListGroup.Item>\n              <Button\n                type='button'\n                className='btn-block'\n                disabled={cartItems.length === 0}\n                onClick={submitHandler}\n              >\n                Order\n              </Button>\n            </ListGroup.Item>\n          </ListGroup>\n        </Card>\n      </Col>\n      <Col md={3}>\n        <Button type='button' className='btn-block' onClick={redirectHandler}>\n          Back to shopping\n        </Button>\n      </Col>\n    </Row>\n  );\n};\n\nexport default CartScreen;\n","E:\\bestdeveloper\\webshop\\frontend\\src\\reducers\\cartReducers.js",["100"],"import { CART_ADD, CART_REMOVE } from '../constants/cartConstants';\n\nexport const cartReducer = (state = { cartItems: [] }, action) => {\n  switch (action.type) {\n    case CART_ADD:\n      //payload\n      const item = action.payload;\n\n      // checking if the item is available\n      const isAvailable = state.cartItems.find(i => i.product === item.product);\n\n      // checking\n      if (isAvailable) {\n        return {\n          ...state,\n          cartItems: state.cartItems.map(i =>\n            i.product === isAvailable.product ? item : i\n          ),\n        };\n      } else {\n        return {\n          ...state,\n          cartItems: [...state.cartItems, item],\n        };\n      }\n      case CART_REMOVE:\n        return {\n        ...state,\n        cartItems: state.cartItems.filter(item => item.product !== action.payload)\n      }\n    default:\n      return state;\n      break;\n  }\n};\n","E:\\bestdeveloper\\webshop\\frontend\\src\\constants\\cartConstants.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\actions\\cartActions.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\Message.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\Loader.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\reducers\\userReducers.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\constants\\userConstants.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\screens\\LoginScreen.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\components\\FormSkeleton.js",[],"E:\\bestdeveloper\\webshop\\frontend\\src\\actions\\userActions.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":10,"column":3,"nodeType":"107","messageId":"108","endLine":10,"endColumn":12},{"ruleId":"109","severity":1,"message":"110","line":33,"column":7,"nodeType":"111","messageId":"112","endLine":33,"endColumn":13},"no-native-reassign",["113"],"no-negated-in-lhs",["114"],"no-unused-vars","'Container' is defined but never used.",